<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="message" format="string" />
    <attr name="time" format="string" />

    <declare-styleable name="CustomEmojiView">
        <attr name="emoji" format="string" />
        <attr name="count" format="string" />
        <attr name="countTextColor" format="color" />
        <attr name="countTextSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="MyMessageViewGroup">
        <attr name="message" />
        <attr name="time" />
    </declare-styleable>
    <declare-styleable name="AlienMessageViewGroup">
        <attr name="avatar" format="string" />
        <attr name="name" format="string" />
        <attr name="message" />
        <attr name="time" />
    </declare-styleable>
    <declare-styleable name="FlexBoxLayout">
        <attr name="horizontalMargins" format="dimension" />
        <attr name="verticalMargins" format="dimension" />
        <attr name="percent" format="float" />
    </declare-styleable>
</resources>
